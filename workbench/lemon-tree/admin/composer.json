{
	"name": "lemon-tree/admin",
	"description": "Laravel + Angular.js admin",
	"authors": [
		{
			"name": "Denis Shumeev",
			"email": "denis-shumeev@yandex.ru"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"illuminate/support": "4.2.*",
		"cartalyst/sentry": "2.1.*",
		"firebase/php-jwt": "dev-master",
		"barryvdh/laravel-ide-helper": "1.*"
	},
	"autoload": {
		"classmap": [
			"src/lib",
			"src/lib/properties",
			"src/controllers",
			"src/migrations",
			"src/seeds",
			"src/views"
		],
		"psr-0": {
			"LemonTree\\Admin\\": "src/"
		}
	},
	"minimum-stability": "stable"
}
